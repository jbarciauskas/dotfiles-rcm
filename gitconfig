[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    type = cat-file -t
    dump = cat-file -p[branch]
    autosetupmerge = true
    autosetuprebase = always
[push]
    default = simple
[core]
    precomposeunicode = false
    editor = /usr/bin/vim
